{
  "name": "iamturns.com",
  "version": "0.0.0",
  "author": "Matt Turnbull <matt@iamturns.com> (https://iamturns.com)",
  "scripts": {
    "start": "next start",
    "dev": "next dev",
    "format": "bash bin/format.sh",
    "lint": "run-p lint-es lint-ts lint-md",
    "lint-es": "eslint --ext .js,.jsx,.ts,.tsx --format=pretty ./",
    "lint-md": "markdownlint '**/*.md'",
    "lint-ts": "tsc --noEmit",
    "test": "jest --passWithNoTests",
    "test--coverage": "npm run test -- --coverage",
    "test--watch": "npm run test -- --watch",
    "build": "next build",
    "validate": "run-p --print-label lint test build && npm start",
    "pkg:example-feature:build": "run-p pkg:hello:build-*",
    "pkg:example-feature:build-src": "cd packages/hello && node bin/build.js",
    "pkg:example-feature:build-ts": "cd packages/hello && tsc --emitDeclarationOnly"
  },
  "dependencies": {
    "next": "10.0.6",
    "react": "17.0.1",
    "react-dom": "17.0.1"
  },
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/dotenv": "^8.2.0",
    "@types/globby": "9.1.0",
    "@types/jest": "^26.0.20",
    "@types/node": "14.14.27",
    "@types/react": "17.0.2",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "doctoc": "^2.0.0",
    "eslint": "^7.20.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-formatter-pretty": "^4.0.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.5",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-unicorn": "^28.0.2",
    "globby": "11.0.2",
    "gray-matter": "4.0.2",
    "import-sort-cli": "^6.0.0",
    "import-sort-parser-babylon": "^6.0.0",
    "import-sort-parser-typescript": "^6.0.0",
    "import-sort-style-module": "^6.0.0",
    "jest": "^26.6.3",
    "markdownlint-cli": "^0.26.0",
    "next-mdx-remote": "2.1.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "sort-package-json": "^1.49.0",
    "typescript": "4.1.5"
  }
}
